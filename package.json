{
  "name": "wp-statistics",
  "version": "1.0.0",
  "description": "The Most Popular Privacy-Friendly Analytics Plugin",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "wp-scripts build --webpack-copy-php --webpack-src-dir=assets/dev/blocks --output-path=assets/blocks",
    "start": "wp-scripts start --webpack-copy-php --webpack-src-dir=assets/dev/blocks --output-path=assets/blocks",
    "build:legacy": "vite build --config vite.config.legacy.js",
    "watch:legacy": "vite build --config vite.config.legacy.js --watch",
    "dev:legacy": "vite build --config vite.config.legacy.js --watch --mode development",
    "build:react": "tsc -b tsconfig.react.json && vite build --config vite.config.react.ts",
    "watch:react": "tsc -b tsconfig.react.json && vite build --config vite.config.react.ts --watch",
    "dev:react": "vite --config vite.config.react.ts",
    "preview:react": "vite preview --config vite.config.react.ts",
    "lint:react": "eslint resources/react/src --config eslint.config.react.js",
    "lint:react:fix": "eslint resources/react/src --config eslint.config.react.js --fix",
    "format:react": "prettier --write \"resources/react/**/*.{ts,tsx,js,jsx,json,css,scss,md}\" --config .prettierrc",
    "format:react:check": "prettier --check \"resources/react/**/*.{ts,tsx,js,jsx,json,css,scss,md}\" --config .prettierrc",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build"
  },
  "dependencies": {
    "@babel/core": "^7.24.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.1.14",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-router": "^1.133.28",
    "@tanstack/react-router-devtools": "^1.132.37",
    "axios": "^1.12.2",
    "chart.js": "^4.4.3",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-adapter-moment": "^1.0.1",
    "chartjs-chart-matrix": "^2.0.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.544.0",
    "moment": "^2.30.1",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^19.1.1",
    "select2": "4.1.0-rc.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.4",
    "@eslint/js": "^9.36.0",
    "@storybook/react": "^10.0.3",
    "@storybook/react-vite": "^10.0.3",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/eslint-plugin-router": "^1.133.19",
    "@tanstack/router-plugin": "^1.132.37",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "@wordpress/i18n": "^6.7.0",
    "@wordpress/scripts": "^27.9.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "glob": "^11.0.0",
    "globals": "^16.4.0",
    "lightningcss": "^1.28.3",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.6.2",
    "readable-stream": "^4.5.2",
    "rtlcss": "^4.2.0",
    "sass": "^1.70.0",
    "sass-embedded": "^1.83.0",
    "sass-migrator": "^2.4.2",
    "shadcn": "^3.4.2",
    "storybook": "^10.0.3",
    "terser": "^5.37.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "npm:rolldown-vite@7.1.12"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.1.12"
    }
  }
}
