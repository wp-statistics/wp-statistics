{
  "name": "wp-statistics",
  "version": "1.0.0",
  "description": "The Most Popular Privacy-Friendly Analytics Plugin",
  "type": "module",
  "private": true,
  "sideEffects": [
    "*.css",
    "*.scss",
    "**/mini-chart.js",
    "**/tracker.js",
    "**/exports.ts",
    "**/custom-components.ts"
  ],
  "scripts": {
    "build": "npm run build:entries && npm run build:blocks && npm run build:react",
    "build:entries": "vite build --config vite.config.entries.js",
    "build:blocks": "vite build --config vite.config.blocks.js",
    "build:react": "vite build --config vite.config.react.ts",
    "start": "npm run dev",
    "dev": "npm run dev:react",
    "dev:entries": "vite build --config vite.config.entries.js --watch --mode development",
    "dev:react": "vite --config vite.config.react.ts",
    "watch": "npm run dev:entries",
    "watch:entries": "vite build --config vite.config.entries.js --watch",
    "watch:blocks": "vite build --config vite.config.blocks.js --watch",
    "watch:react": "vite build --config vite.config.react.ts --watch",
    "preview:react": "vite preview --config vite.config.react.ts",
    "lint:react": "eslint resources/react/src --config eslint.config.react.js",
    "lint:react:fix": "eslint resources/react/src --config eslint.config.react.js --fix",
    "lint:analytics": "pnpm --package=@ast-grep/cli dlx sg scan resources/react/src --config resources/react/.ast-grep/sgconfig.yml",
    "format:react": "prettier --write \"resources/react/**/*.{ts,tsx,js,jsx,json,css,scss,md}\" --config .prettierrc",
    "format:react:check": "prettier --check \"resources/react/**/*.{ts,tsx,js,jsx,json,css,scss,md}\" --config .prettierrc",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "test:storybook": "vitest --project=storybook --run",
    "test:storybook:watch": "vitest --project=storybook",
    "test:storybook:ci": "vitest --project=storybook --run",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report tests/e2e/reports"
  },
  "dependencies": {
    "@babel/core": "^7.24.7",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.1.14",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-router": "^1.133.28",
    "@tanstack/react-router-devtools": "^1.132.37",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.544.0",
    "react": "^19.2.0",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.0",
    "react-simple-maps": "^3.0.0",
    "recharts": "2.15.4",
    "select2": "4.1.0-rc.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.4",
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "^10.1.9",
    "@storybook/addon-vitest": "^10.1.11",
    "@storybook/react": "^10.1.11",
    "@storybook/react-vite": "^10.1.10",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/eslint-plugin-router": "^1.133.19",
    "@tanstack/router-plugin": "^1.132.37",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-simple-maps": "^3.0.6",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "@wordpress/i18n": "^6.7.0",
    "@wordpress/scripts": "^27.9.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.36.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "glob": "^11.0.0",
    "globals": "^16.4.0",
    "jsdom": "^27.4.0",
    "lightningcss": "^1.28.3",
    "msw": "^2.12.4",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.6.2",
    "readable-stream": "^4.5.2",
    "rtlcss": "^4.2.0",
    "sass": "^1.70.0",
    "sass-embedded": "^1.83.0",
    "sass-migrator": "^2.4.2",
    "shadcn": "^3.4.2",
    "storybook": "^10.1.11",
    "storybook-addon-rtl": "^3.0.1",
    "terser": "^5.37.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.16"
  },
  "msw": {
    "workerDirectory": [
      ".storybook/public"
    ]
  }
}
