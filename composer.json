{
  "require": {
    "php": ">=7.4",
    "geoip2/geoip2": "^2.13",
    "matomo/device-detector": "^6.4",
    "deliciousbrains/wp-background-processing": "^1.4"
  },
  "config": {
    "platform-check": false,
    "platform": {
      "php": "7.4"
    }
  },
  "autoload": {
    "psr-4": {
      "WP_Statistics\\": "src/"
    },
    "classmap": [
      "src/Dependencies/deliciousbrains/wp-background-processing/classes/",
      "vendor/mustangostang/spyc/Spyc.php"
    ]
  },
  "extra": {
    "mozart": {
      "dep_namespace": "WP_Statistics\\Dependencies\\",
      "dep_directory": "/src/Dependencies/",
      "classmap_directory": "/src/Dependencies/",
      "classmap_prefix": "WP_Statistics_",
      "packages": [
        "geoip2/geoip2",
        "psr/cache",
        "matomo/device-detector",
        "deliciousbrains/wp-background-processing"
      ],
      "delete_vendor_directories": true
    }
  },
  "scripts": {
    "test": "phpunit --testdox",
    "test:unit": "phpunit --testdox --testsuite unit",
    "test:integration": "phpunit --testdox --testsuite integration",
    "test:coverage": "phpunit --coverage-html tests/coverage",
    "test:react": "pnpm test:run",
    "test:react:watch": "pnpm test",
    "test:react:coverage": "pnpm test:coverage",
    "test:e2e": "pnpm test:e2e",
    "test:storybook": "pnpm test:storybook",
    "lint": "pnpm lint:react",
    "lint:fix": "pnpm lint:react:fix",
    "format": "pnpm format:react",
    "format:check": "pnpm format:react:check",
    "build": "pnpm build",
    "dev": "pnpm dev",
    "storybook": "pnpm storybook",
    "check": [
      "@test",
      "@lint",
      "@format:check"
    ],
    "check:all": [
      "@test",
      "@test:react",
      "@lint",
      "@format:check"
    ],
    "post-install-cmd": [
      "PATH=$(composer global config bin-dir --absolute --quiet):$PATH mozart compose",
      "composer dump-autoload"
    ],
    "post-update-cmd": [
      "PATH=$(composer global config bin-dir --absolute --quiet):$PATH mozart compose",
      "composer dump-autoload"
    ]
  }
}
