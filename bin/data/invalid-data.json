    "resource_id": {
      "strings": ["not_a_number", "12abc", "id:123", ""],
      "floats": ["1.5", "99.99", "-0.1"],
      "special": ["null", "undefined", "NaN"]
    },
    "screenWidth": {
      "strings": ["wide", "100%", "auto", "1920px", "calc(100vw)"],
      "invalid_numbers": ["1e10", "0x7F", "0b1010"]
    },
    "screenHeight": {
      "strings": ["tall", "100vh", "fit-content", "1080px"],
      "invalid_numbers": ["-1e10", "0xFFFF"]
    }
  },

  "base64_corruption": {
    "invalid_base64": [
      "not-valid-base64!!!",
      "YWJj====",
      "YWJj==extra",
      "!!!YWJj",
