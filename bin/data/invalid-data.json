  "type_mismatches": {
    "resourceUriId": {
      "strings": ["abc", "12.5", "null", "undefined", "NaN", "Infinity"],
      "arrays": "[[1, 2, 3]]",
      "objects": "{\"id\": 1}",
      "booleans": ["true", "false"]
