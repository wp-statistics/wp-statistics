  "empty_and_null_variations": {
    "empty_strings": ["", " ", "  ", "\t", "\n", "\r\n", "\t\n\r"],
    "null_like": ["null", "NULL", "Null", "nil", "None", "undefined"],
    "zero_variations": ["0", "0.0", "00", "0x0", "0b0", "-0"]
